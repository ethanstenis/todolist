{
  "rules": {
      "indent": [1, 4],
      "linebreak-style": [1, "unix"],
      "no-console": 0,
      "no-redeclare": 0,
      "no-unused-vars": 0,
      "quotes": [1, "single"],
      "no-empty": 0,
      "no-undef": 0,
      "no-cond-assign": 0,
      "no-inner-declarations": 0,
      "semi": 0
  },
  "env": {
      "es6": true,
      "browser": true,
      "jquery": true
  },
  "extends": "eslint:recommended"
}
